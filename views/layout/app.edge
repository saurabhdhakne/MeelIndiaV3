  <title>MeelIndia</title>
  <link href="https://fonts.googleapis.com/css2?family=Play&display=swap" rel="stylesheet">

  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
 <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
<style>
      


      
    body {
      margin: 0;
      padding: 0;
      background:#E5E5E5;
      font-family: 'Century Gothic';
      padding-top: 70px;
      padding-bottom: 50px;
    }
    nav {
        font-size: 1.0em;
        font-family: 'Century Gothic', sans-serif;
      }
      
      nav a{
        text-decoration-color: none;
        cursor: pointer;
        color: snow;
      }
  
      nav a:hover{
        color: #BEBEBE;
      }
  
    .shadowme{
      border-radius: 5px;
      box-shadow: 1px 2px 5px black;
      cursor: pointer;
    }

    .pointer{
      cursor: pointer;
    }
    .img-circle{
      border-radius: 50%;
    }
    .products{
      border-radius: 5px;
      box-shadow: 1px 2px 5px black;

    }
    .product{
      border-radius: 5px;
      box-shadow: 1px 2px 5px black;
      border-left: 10px solid snow;
      transition: all .5s ease;
      cursor: pointer;
    }
    .product:hover{
      border-radius: 5px;
      box-shadow: 1px 2px 5px black;
      border-left: 10px solid black;
    }
    .product .details{
        color: grey;      
    }
    .addProduct{
      border-radius: 5px;
      box-shadow: 1px 2px 5px black;
      transition: all .5s ease;
      cursor: pointer;
    }
    .addProduct:hover{
      border-radius: 5px;
      box-shadow: 1px 2px 5px black;
      border-left: 10px solid dodgerblue;
      border-right: 10px solid dodgerblue;
      
    }
    /*Animate btn */
    .abtn{
        border-radius:10px;
        font-family: 'Play', sans-serif;
 
      }
      .abtn:hover{
        color: white;
        background: black;
        -webkit-transition: all .5s;
        border-radius: 50px; /* For Safari 3.0 to 6.0 */
        
      }

      .ytedit{
        background:none; border:none; 
        padding:5px;
        border-radius: 5px;
      }

      .ytedit:hover{
        background: white;
        
      }

      .bgsocial{
        position: absolute;
        top: 50%;
        left: 50%;
        padding:10%;
        -ms-transform: translate(-50%, -50%);
        transform: translate(-50%, -50%);
        
        opacity: .3;
        height: 90%;
        }
    .sidenav {
    height: 100%;
    width: 0;
    font-size: 1.2em;
    position: fixed;
    z-index: 9;
    top: 10%;
    left: 0;
    overflow-x: hidden;
    transition: 0.5s;
    padding-top: 60px;
  }

  .sidenav a {
    padding: 8px 8px 8px 22px;
    text-decoration: none;
    display: block;
    transition: 0.3s;
  }

  .sidenav a:hover {
    color: #f1f1f1;
    padding-left: 32px;
  }

  .sidenav .closebtn {
    position: absolute;
    top: 0;
    right: 25px;
    font-size: 36px;
    margin-left: 50px;
  }
  h2 {
    font-family: Arial, Verdana;
    font-weight: 600;
    font-size: 2.5rem;
    color: #091f2f;
    text-transform: uppercase;
    }
    .accordion-section .panel-default > .panel-heading {
        border: 0;
        background: #091f2f;
        padding: 0;
    }
    .accordion-section .panel-default .panel-title a {
        display: block;
        font-style: italic;
        font-size: 1.2rem;
    }
    .accordion-section .panel-default .panel-title a:after {
        font-family: 'FontAwesome';
        font-style: normal;
        font-size: 2rem;
        content: "\f106";
        color: white;
        float: right;
        margin-top: -12px;
    }
    .accordion-section .panel-default .panel-title a.collapsed:after {
        content: "\f107";
    }
    .accordion-section .panel-default .panel-body {
        font-size: 1rem;
    }

  @media screen and (max-height: 450px) {
    .sidenav {padding-top: 15px;}
    .sidenav a {font-size: 18px;}
  }

      @media (max-width: 768px) {
        nav{
          text-align: right;
        }
              .navbar-brand.abs {
                position: absolute;
                width: 100%;
                left: 0;
                text-align: center;
              }
            
            .navbar-toggler-icon{
                color: snow;
                background: dodgerblue;
                border-radius: 50%;
            }

      }

  @media Screen and (min-width:900px) {
	section{
		padding: 7.9rem;
	}

	.menu-toggle{
		display: none;
	}

	.nav{
		justify-content: space-between;
	}

	.nav-list {
		position: fixed;
		width: 80%;
		height: 100vh;
		background-color: transparent;
		justify-content: space-around;
		flex-direction: row;
    transform: translateX(-100%);
  	transition: transform 0.5s; 
	}

	.nav-item {
		margin: 0 1.2rem;
		border: none;
	}

	.nav-item:last-child {
		margin-right: 0;
	}

	.nav-link {
		font-size: 0.8rem;
	}

	.active {
		position: relative;
	}

	.active::before {
		content: '';
		position: absolute;
		width: 85%;
		height: 2px;
		background-color: #fff;
		bottom: -4px;
		left:0;
	}
      }

    
</style>
</head>

